        <Button x:Name="exportButton" Content="导出" HorizontalAlignment="Right" Height="39" Margin="0,119,10,0" VerticalAlignment="Top" Width="145" Click="exportButton_Click"/>
        <ProgressBar x:Name="progressbar1" HorizontalAlignment="Right" Height="39" Margin="0,119,10,0" VerticalAlignment="Top" Width="145" IsIndeterminate="True" Visibility="Hidden" Background="{x:Null}" BorderBrush="{x:Null}"/>
        <Button x:Name="setExceptionButton" Content="编辑组长排除名单" HorizontalAlignment="Right" Height="39" Margin="0,177,10,0" VerticalAlignment="Top" Width="145" Click="setExceptionButton_Click" />

    </Grid>
