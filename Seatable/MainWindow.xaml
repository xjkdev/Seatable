<Window x:Class="Seatable.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Seatable"
        mc:Ignorable="d"
        Title="MainWindow" Height="533.077" Width="686.539" Closing="Window_Closing" Loaded="Window_Loaded">
    <Grid >
        <Button x:Name="abutton" Content="生成" Height="40" Margin="0,55,10,0" VerticalAlignment="Top" Click="abutton_Click" HorizontalAlignment="Right" Width="145"/>
